{
  "name": "opinionated-people-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:dev": "prisma migrate dev --name init",
    "prisma:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "cors": "2.8.5",
    "date-fns": "^3.6.0",
    "express": "4.21.2",
    "uuid": "11.0.3",
    "ws": "8.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.12",
    "prisma": "5.20.0",
    "tsx": "4.19.2",
    "typescript": "5.7.2"
  }
}
